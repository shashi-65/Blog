<%- include('partials/header'); -%>


<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newText">Title</label> 
        <input type="text" name="newText" class="form-control" placeholder="Enter title"> 
    </div>

    <div class="form-group">
        <label for="postBody">Post</label> 
        <textarea name="postBody" cols="40" rows="5" class="form-control" placeholder="Write your Journal here!"></textarea>
    </div>

    <button type="submit" name="button" class="btn btn-primary">Publish</button>

</form>


<%- include('partials/footer'); -%>